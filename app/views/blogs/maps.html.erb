<% if !current_user %>
  <%= link_to "Sign Up", new_user_path %>
  <%end %>

<div id="address-map"></div>  
<% @blogs.each do |b| %>
  <tr>
    <td><% b.title %></td>
    <td><% b.country %></td>
    <td><% b.description %></td>
    <td><% link_to 'Show', [b] %></td>
  </tr>
  <% end %>
</table>

 



